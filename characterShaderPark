//change colors quickly
//change vertical horizontal positions character
function dominantColor() { color(0.5,0.0,0.1) };
function magenta() { color(0.5,0.0,0.1) };
function hotPink() { color(1.5,0.0,0.1)};
function lavender() { color(1.0, 0.5, 1.0)};
function black() {color(0.0,0.0, 0.0)};
function white() {color(1.0,1.40,2.05)};
function yellow() {color(1.9,1.6,0.0)};
function standard() {color(1.0, 1.0, 1.0)};
function red() {color(1.0, 0.0, 0.0)};
function brown() {color(0.2,0.1,0.05)};
function green() {color(0.02,0.245,0.05)};
