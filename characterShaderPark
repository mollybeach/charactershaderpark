function lightBrown() {color(1.0,0.4,0.03)};
function lightBlue() {color(0.0,1.9,1.8)};
function pink () {color(3.0, 0.5, 1.0)};
function purple () {color(0.1,0.0,0.9)};
let animation = shape( () => {
